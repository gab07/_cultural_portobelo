<p id="notice"><%= notice %></p>

<h1>Listing Contact Forms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Profession</th>
      <th>Comment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contact_forms.each do |contact_form| %>
      <tr>
        <td><%= contact_form.name %></td>
        <td><%= contact_form.email %></td>
        <td><%= contact_form.phone %></td>
        <td><%= contact_form.profession %></td>
        <td><%= contact_form.comment %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact form', new_contact_form_path %>
